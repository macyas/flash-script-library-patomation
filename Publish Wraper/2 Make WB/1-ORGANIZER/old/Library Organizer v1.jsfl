/** * Organizes all library items - folders get  * created for each item type and items get moved into those folders. *///possible item types://undefined,component,movie clip,//graphic,button,folder,font,//sound,bitmap,compiled clip,screen,videovar lib = fl.getDocumentDOM().library;function moveBitmap(item){lib.moveToFolder("Bitmaps & JPEGs",item.name,false);}function moveSound(item){lib.moveToFolder("Audio",item.name,false);}function moveVideo(item){lib.moveToFolder("Video",item.name,false);}var items = fl.getDocumentDOM().library.items;var i=0;var l=items.length;var missed = false;for(i;i<l;i++){	var item = items[i];	if(item.name.indexOf("bitmaps")>-1) continue;	if(item.name.indexOf("sounds")>-1) continue;	if(item.name.indexOf("videos")>-1) continue;	switch(items[i].itemType)	{		case "undefined":			continue;		case "bitmap":			moveBitmap(item);			break;		case "sound":			moveSound(item);			break;		case "video":			moveVideo(item);			break;		case "folder":			continue;	}}lib.selectNone();lib.editItem();if(missed) alert("Some items could not be organized.");
//// Image Scale Alignment//// This command was recorded by Adobe Flash CS6.//// Move: {x:1000, y:1000}fl.getDocumentDOM().moveSelectionBy({x:1000, y:1000});// Scale: 0.34, 0.34fl.getDocumentDOM().scaleSelection(0.34, 0.34);// Select Allfl.getDocumentDOM().selectAll();// Match Size: true, true, falsefl.getDocumentDOM().match(true, true, false);// Align: left, falsefl.getDocumentDOM().align('left', false);// Align: top, falsefl.getDocumentDOM().align('top', false);// Distribute to Layersfl.getDocumentDOM().distributeToLayers();// Lock Layers: truefl.getDocumentDOM().getTimeline().setLayerProperty('locked', true);// Deletefl.getDocumentDOM().deleteSelection();
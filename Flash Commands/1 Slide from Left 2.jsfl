//// 1 Slide from Left 2//// This command was recorded by Adobe Flash CS6.//// Move: {x:-90, y:0}fl.getDocumentDOM().moveSelectionBy({x:-90, y:0});// Scale: 1.20, 1.20fl.getDocumentDOM().scaleSelection(1.20, 1.20);// Add Filter: blurFilterfl.getDocumentDOM().addFilter('blurFilter')// Instance Alpha: 0%fl.getDocumentDOM().setInstanceAlpha(0);// Create Classic Tweenfl.getDocumentDOM().getTimeline().createMotionTween();// Frame Property: tweenEasing, 100fl.getDocumentDOM().getTimeline().setFrameProperty('tweenEasing', 100);
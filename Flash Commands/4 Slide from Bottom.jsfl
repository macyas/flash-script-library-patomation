//// 4 Slide from Bottom//// This command was recorded by Adobe Flash CS6.//// Move: {x:0, y:90}fl.getDocumentDOM().moveSelectionBy({x:0, y:90});// Add Filter: blurFilterfl.getDocumentDOM().addFilter('blurFilter')// Instance Alpha: 0%fl.getDocumentDOM().setInstanceAlpha(0);// Create Classic Tweenfl.getDocumentDOM().getTimeline().createMotionTween();// Frame Property: tweenEasing, 100fl.getDocumentDOM().getTimeline().setFrameProperty('tweenEasing', 100);
//// 1 Slide from Left//// This command was recorded by Adobe Flash CS6.//// Move: {x:-60, y:0}fl.getDocumentDOM().moveSelectionBy({x:-60, y:0});// Scale: 1.38, 1.38fl.getDocumentDOM().scaleSelection(1.38, 1.38);// Remove All Filtersfl.getDocumentDOM().removeAllFilters()// Instance Alpha: 0%fl.getDocumentDOM().setInstanceAlpha(0);// Create Classic Tweenfl.getDocumentDOM().getTimeline().createMotionTween();// Frame Property: tweenEasing, 100fl.getDocumentDOM().getTimeline().setFrameProperty('tweenEasing', 100);// Clear Frame Selectionfl.getDocumentDOM().getTimeline().setSelectedFrames([]);